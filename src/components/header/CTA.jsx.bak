import React from 'react';

// External resumeink (OneDrive). Clicking opens the resume in a new tab.
const CV = 'https://1drv.ms/w/c/F0BA73D8625C7225/EdEiiHOm9ZFCjqp3jU1lUoYBx381_3qHRdVwOGH1fVtUsw?e=0Ze2xi';

const CTA = () => {
  return (
    <div className="cta">
      <a href={CV} target="_blank" rel="noopener noreferrer" className="btn">
        Resume
      </a>
      <a href="#contact" className="btn btn-primary">
        Let's talk
      </a>
    </div>
  );
};

export default CTA;
